# SELECT NAME, COUNT(*) AS `COUNT`
# FROM ANIMAL_INS
# WHERE NAME IN (SELECT NAME
#               FROM ANIMAL_INS
#               WHERE NAME IS NOT NULL
#               GROUP BY NAME
#               HAVING COUNT(*) >= 2)
# ORDER BY NAME;

SELECT NAME, COUNT(*) AS `COUNT`
FROM ANIMAL_INS
WHERE NAME IN (SELECT NAME
              FROM ANIMAL_INS
              WHERE NAME IS NOT NULL
              GROUP BY NAME
              HAVING COUNT(*) >= 2)
GROUP BY NAME
ORDER BY NAME;